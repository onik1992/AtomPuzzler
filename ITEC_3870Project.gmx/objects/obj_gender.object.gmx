        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Change the Player's Sprite Set

/*If the player touches this object when
the touch variable is true, it will change 
their sprites accordingly and set the touch
variable to false. It will also play the select sound effect.
*/

// emulate a tap/click as the up key
if(mobile)
{
    with(obj_player)
    {
        if(input.tap &amp;&amp; mouse_check_button_released(mb_left))
        {
            with(obj_gender)
            {
                    keyboard_key_press(vk_up);
            } 
            keyboard_key_release(vk_up);
        }
    }
}

if(touch &amp;&amp; mainPass)
{
    
    room_goto_transition(rm_mainMenu, transition.stripe_ver, 60);
    
    switch(obj_player.spr)
    {
        case "boy1":
        {
            obj_player.spr = "girl1";
            touch = false;
            if(debug_mode)
            {
                show_message("Changed to the female character");
                show_debug_message("Changed to the female character");
            }
            break;
        }
        case "girl1":
        {
            obj_player.spr = "boy1";
            touch = false;
            if(debug_mode)
            {
                show_message("Changed to the male character");
                show_debug_message("Changed to the male character");
            }
            break;
        }
    }
    
    audio_play_sound(sfx_Select, 0, false);
    
}
</string>
